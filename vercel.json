{
  "version": 2,
  "buildCommand": "cd frontend && yarn install && yarn build",
  "outputDirectory": "frontend/build",
  "installCommand": "yarn install",
  "routes": [
    {
      "src": "/pos.html",
      "dest": "/pos.html"
    },
    {
      "src": "/receipt.html",
      "dest": "/receipt.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
